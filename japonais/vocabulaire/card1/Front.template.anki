<script>
function shuffleList() {
var list = document.querySelector('ol') || document.querySelector('ul');
if (list) {
    for (var i = list.children.length; i >= 0; i--) {
        list.appendChild(list.children[Math.floor(Math.random() * i)]);
    }
} 
}

shuffleList();
</script>